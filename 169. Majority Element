int majorityElement(int* nums, int numsSize) {
    int candidate=0;
    int counter=0;
    int i=0;
    if(numsSize==1){
        return *nums;
    }
    for(;i<numsSize;i++){
        
        if( *(nums+i)==candidate ){
            counter++;
        }
        else if(counter==0){
            candidate=*(nums+i);
            counter=1;
        }
        else{
            counter--;
        }
    }
    return candidate;
    
}
